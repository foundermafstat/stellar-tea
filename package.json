{
  "name": "scaffold-stellar-frontend",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.6.2",
  "private": true,
  "scripts": {
    "dev": "concurrently \"stellar scaffold watch\" \"vite\"",
    "start": "concurrently \"stellar scaffold watch\" \"vite\"",
    "build": "tsc -b && vite build",
    "install:contracts": "pnpm --filter \"./packages/*\" install && pnpm --filter \"./packages/*\" run --if-present build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier . --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@creit.tech/stellar-wallets-kit": "^1.9.5",
    "@fontsource-variable/montserrat": "^5.2.8",
    "@fontsource/sour-gummy": "^5.2.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stellar/design-system": "^3.1.5",
    "@stellar/stellar-sdk": "^14.2.0",
    "@stellar/stellar-xdr-json": "^23.0.0",
    "@tanstack/react-query": "^5.90.2",
